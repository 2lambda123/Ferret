set wind/asp=1:ax/size=1 1
 
define view/x=0.00,0.25/y=0.00,0.25 v11
define view/x=0.25,0.50/y=0.00,0.25 v12
define view/x=0.50,0.75/y=0.00,0.25 v13
define view/x=0.75,1.00/y=0.00,0.25 v14
