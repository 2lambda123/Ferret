! Check that an axis in use cannot be cancelled.

use test_axes
cancel data test_axes

let tt = t[gt=test_irreg] + 50*x[i=1:10]
load tt

show axis month*
cancel axis test_irreg

show axis month*

! Pre V5.42, this causes a crash: axes were cancelled, but not fully.
cancel axis/all
show axis month*

cancel var tt
cancel axis/all

show axis month*
