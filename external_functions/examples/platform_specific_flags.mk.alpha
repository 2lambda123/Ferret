#
# platform_specific_includes.mk.alpha
#
# This file is included in the External Function Makefiles and defines
# platform specific macros

CCSHFLAG        =
CC              = cc
RANLIB          = /usr/bin/ranlib
CFLAGS          = -g $(CCSHFLAG)
F77             = f77
F77SHFLAG       = -shared
FFLAGS          = -g -align dcommons -align records -integer_size 32 -real_size 32 -cpp \
		  -u -fpe2 -C $(F77SHFLAG)
LD_DYN_FLAGS    = -shared
