! benchmark script for convolvei external function     

let weight = IF I[I=1:3] EQ 3 THEN 1 ELSE I[I=1:3]
let wnorm = weight/4
let a = sin(x[x=1:20:1])

list a,convolvei(a,wnorm)

plot a,convolvei(a,wnorm)

